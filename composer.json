{
    "name": "laravel-hyperf/bus",
    "description": "The bus package for Laravel Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "bus",
        "queue",
        "swoole",
        "laravel-hyperf"
    ],
    "support": {
        "issues": "https://github.com/laravel-hyperf/components/issues",
        "source": "https://github.com/laravel-hyperf/components"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com",
            "homepage": "https://albert-chen.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "hyperf/context": "~3.1.0",
        "hyperf/contract": "~3.1.0",
        "hyperf/collection": "~3.1.0",
        "hyperf/conditionable": "~3.1.0",
        "hyperf/coroutine": "~3.1.0",
        "hyperf/arrayable": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "nesbot/carbon": "^3.0",
        "laravel/serializable-closure": "^1.3",
        "laravel-hyperf/support": "~0.1.0",
        "laravel-hyperf/cache": "~0.1.0"

    },
    "autoload": {
        "psr-4": {
            "LaravelHyperf\\Bus\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "extra": {
        "hyperf": {
            "config": "LaravelHyperf\\Bus\\ConfigProvider"
        },
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "suggest": {
        "laravel-hyperf/queue": "Required to use closures when chaining jobs."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}